{
	"name": "steam-gifter",
	"version": "2.1.0",
	"license": "MIT",
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"scripts": {
		"tsc": "tsc",
		"graphql-codegen": "graphql-codegen",
		"wait-for-it": "wait-for-it",
		"prisma": "prisma",
		"prisma:generate": "prisma generate",
		"nx:update": "nx migrate latest",
		"build:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && npx nx run-many --target=build --all",
		"docker:down:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && sh ./docker/docker-compose-down.sh",
		"docker:restart:prod": "npm run docker:down:prod && npm run docker:up:prod",
		"docker:up:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && sh ./docker/docker-compose-up.sh",
		"docker:clean-down:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && sh ./docker/docker-compose-clean-down.sh",
		"docker:clean-restart:prod": "npm run docker:clean-down:prod && npm run docker:clean-up:prod",
		"docker:clean-up:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && sh ./docker/docker-compose-clean-up.sh",
		"database:migrate:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && export POSTGRES_URL=$PRISMA_POSTGRES_URL && npx prisma migrate dev --skip-seed",
		"database:recreate:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && export POSTGRES_URL=$PRISMA_POSTGRES_URL && npx prisma migrate reset --skip-seed",
		"database:seed:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && export POSTGRES_URL=$PRISMA_POSTGRES_URL && npx prisma db seed -- --environment $NODE_ENV",
		"prisma:studio:prod": "export $(xargs < ./env/.env.prod) > /dev/null 2>&1 && export POSTGRES_URL=$PRISMA_POSTGRES_URL && npx prisma studio --port 5551",
		"generate:ci": "prisma generate && npm run graphql-codegen",
		"--------------TOOLS---------------": "",
		"update": "node --experimental-modules tools/scripts/updater.mjs",
		"build": "export $(xargs < ./env/.env.build) > /dev/null 2>&1 && npx nx run-many --target=build --all",
		"create-archive": "node --experimental-modules ./tools/scripts/create-archive.mjs",
		"upload-update": "node --experimental-modules ./tools/scripts/upload-update.mjs",
		"------------SERVER DEV-------------": "",
		"server:start:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx nx serve server --configuration=$NODE_ENV",
		"server:repl:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx nx run server:repl --configuration=$NODE_ENV",
		"-----------CLIENT DEV-------------": "",
		"client:start:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx nx serve client --configuration=$NODE_ENV --port 4200",
		"client-panel:start:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx nx serve client-panel --configuration=$NODE_ENV --port 4201",
		"------------PRISMA DEV-------------": "",
		"database:migrate:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma migrate dev --skip-seed",
		"database:migrate:create:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma migrate dev --create-only --skip-seed",
		"database:recreate:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma migrate reset --preview-feature --skip-seed",
		"database:pull:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma db pull && prisma generate",
		"database:push:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma db push",
		"database:seed:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma db seed -- --environment $NODE_ENV",
		"prisma:studio:dev": "export $(xargs < ./env/.env.dev) > /dev/null 2>&1 && npx prisma studio --port 5551"
	},
